create table peliculas (
m_id INT NOT NULL PRIMARY KEY,
m_returned INT NOT NULL PRIMARY KEY,
n_socio INT NOT NULL PRIMARY KEY,
f_regreso INT NOT NULL PRIMARY KEY,
f_tran INT NOT NULL PRIMARY KEY,
n_referencia INT NOT NULL PRIMARY KEY
);

insert into peliculas ( m_id, m_returned, n_socio, f_regreso, f_tran, n_referencia) values (1, 0, 1, NULL, 20-06-2012, 11);
