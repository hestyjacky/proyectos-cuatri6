create table hola(
h1 int,
h2 varchar,
h3 date
);

show tables;
insert into hola (h1, h2, h3) values (11, 22, 33);
select h2 from hola;
select h3 from hola where h3 = 66;
select * from hola where h3 = 66;
select * from hola where h3 = 6;
select h2, h3 from hola where h3 = 66;
select h2, h3 from hola;
select nombre, salario from empleados;
select nombre, id from empleados where id = 25;

SELECT h1 from hola;
select distinct h1 from hola;

select min (h1) from hola;
select min(h1) from hola;

SELECT MAX (h1) FROM hola;
SELECT MAX(h1) FROM hola;

